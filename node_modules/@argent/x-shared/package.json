{
  "name": "@argent/x-shared",
  "version": "1.56.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/argentlabs/x-shared.git"
  },
  "license": "MIT",
  "private": false,
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    },
    "./simulation": {
      "types": "./dist/simulation.d.ts",
      "import": "./dist/simulation.js",
      "default": "./dist/simulation.cjs"
    },
    "./swap": {
      "types": "./dist/swap.d.ts",
      "import": "./dist/swap.js",
      "default": "./dist/swap.cjs"
    }
  },
  "scripts": {
    "build": "vite build",
    "dev": "NODE_ENV=development vite build --mode development --watch",
    "lint": "eslint . --cache --ext .ts,.tsx",
    "lint:fix": "pnpm lint --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepare": "husky",
    "format:fix": "prettier . --ignore-unknown --write"
  },
  "devDependencies": {
    "@amplitude/analytics-types": "2.8.4",
    "@commitlint/cli": "19.6.1",
    "@commitlint/config-conventional": "19.6.0",
    "@rollup/plugin-typescript": "12.1.2",
    "@semantic-release/git": "10.0.1",
    "@types/async-retry": "1.4.9",
    "@types/lodash-es": "4.17.12",
    "@types/ua-parser-js": "0.7.39",
    "@typescript-eslint/eslint-plugin": "8.18.0",
    "@typescript-eslint/parser": "8.18.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.31.0",
    "husky": "9.1.7",
    "lint-staged": "15.2.11",
    "msw": "2.6.8",
    "prettier": "3.4.2",
    "semantic-release": "24.2.0",
    "ts-to-zod": "3.15.0",
    "typescript": "5.7.2",
    "vite": "6.0.3",
    "vite-plugin-dts": "4.3.0",
    "vitest": "2.1.8"
  },
  "dependencies": {
    "@argent/x-multicall": "^7.1.0",
    "@noble/hashes": "^1.3.1",
    "@starknet-io/types-js": "^0.7.7",
    "big-decimal-number": "npm:bignumber.js@^9.1.2",
    "micro-starknet": "^0.2.3",
    "starknet": "6.11.0",
    "starknet4-deprecated": "npm:starknet@4.4.0",
    "starknet5": "npm:starknet@5.25.0"
  },
  "peerDependencies": {
    "@scure/base": "^1.1.1",
    "@scure/bip39": "^1.2.1",
    "async-retry": "^1.3.3",
    "lodash-es": "^4.17.21",
    "ua-parser-js": "^1.0.38 || ^2.0.0",
    "url-join": "^5.0.0",
    "zod": "^3.23.8"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write",
    "*.{ts,tsx}": "eslint --cache --fix"
  }
}
